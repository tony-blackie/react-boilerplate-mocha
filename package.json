    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "browser-sync": "^2.18.2",
