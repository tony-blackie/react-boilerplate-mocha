    "cross-env": "^3.1.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-minifier": "^3.2.2",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "normalize.css": "^4.2.0",
